{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Next: Launch",
      "runtimeExecutable": "npm",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["--inspect", "run", "dev"],
      "port": 9229,
      "console": "integratedTerminal",
      "timeout": 100000
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Next: Attach",
      "port": 9229,
      "stopOnEntry": false,
      "restart": true,
      "timeout": 100000
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Next: Chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}"
    }
  ],
  "compounds": [
    {
      "name": "Next: Full",
      "configurations": ["Next: Launch", "Next: Attach", "Next: Chrome"]
    }
  ]
}
